data.sort(key=lambda row: row[4], reverse=True)
for i in range(len(data)):
    test = data[i][4]
    data[i].append(test)

print('Имя              |        Сум')
print('_________________________________________________________')
for row in data[:20]:
    print('{: <16} | {: >10.0f}'.format(row[0], row[4]))



